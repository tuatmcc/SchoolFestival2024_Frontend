{
	"name": "schoolfestival2024-frontend",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "remix vite:build",
		"dev": "remix vite:dev",
		"start": "vite preview",
		"typecheck": "tsc",
		"lint": "biome check .",
		"fmt": "biome check . --write --unsafe",
		"prepare": "partytown copylib public/~partytown",
		"gen:type": "supabase gen types --lang=typescript --local > app/libs/database.ts",
		"gen:seed": "tsx seed.ts > supabase/seed.sql",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"dependencies": {
		"@builder.io/partytown": "0.10.2",
		"@radix-ui/react-slot": "1.1.0",
		"@remix-run/react": "2.13.1",
		"@supabase/supabase-js": "2.45.4",
		"class-variance-authority": "0.7.0",
		"clsx": "2.1.1",
		"lucide-react": "0.453.0",
		"qrcode.react": "4.0.1",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-hook-form": "7.53.0",
		"swr": "2.2.5",
		"tailwind-merge": "2.5.4",
		"tailwindcss-animate": "1.0.7",
		"type-fest": "4.26.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.3",
		"@remix-run/dev": "2.13.1",
		"@snaplet/copycat": "5.1.0",
		"@snaplet/seed": "0.98.0",
		"@storybook/addon-a11y": "8.3.5",
		"@storybook/addon-actions": "8.3.5",
		"@storybook/addon-essentials": "8.3.5",
		"@storybook/addon-interactions": "8.3.5",
		"@storybook/addon-links": "8.3.5",
		"@storybook/blocks": "8.3.5",
		"@storybook/react": "8.3.5",
		"@storybook/react-vite": "8.3.5",
		"@storybook/test": "8.3.5",
		"@types/react": "18.3.11",
		"@types/react-dom": "18.3.1",
		"autoprefixer": "10.4.20",
		"postcss": "8.4.47",
		"postgres": "3.4.4",
		"storybook": "8.3.5",
		"supabase": "1.204.3",
		"tailwindcss": "3.4.13",
		"tsx": "4.19.1",
		"typescript": "5.6.3",
		"vite": "5.4.8",
		"vite-tsconfig-paths": "5.0.1"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
